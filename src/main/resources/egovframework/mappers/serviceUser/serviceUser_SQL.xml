<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="egovframework.user.service.impl.sericeUserMapper">
  
  <select id="duplicatedEmailCheck" parameterType="signUpVO" resultType="int">
  	SELECT COUNT(*) FROM SERVICE_USER
  	WHERE EMAIL = #{email}
  </select>
  
  
</mapper>